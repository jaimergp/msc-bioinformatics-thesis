keywords = {Drug Design,Hydrogen Bonding,Ligands,Molecular Docking Simulation,Protein Conformation,Proteins,Proteins: chemistry,Proteins: metabolism,Structure-Activity Relationship,Thrombin,Thrombin: chemistry,Thrombin: metabolism,Water,benchmark,p38 Mitogen-Activated Protein Kinases,p38 Mitogen-Activated Protein Kinases: chemistry,p38 Mitogen-Activated Protein Kinases: metabolism},
mendeley-tags = {benchmark},
month = jan,
number = {1},
