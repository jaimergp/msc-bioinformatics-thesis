journal = {Journal of computer-aided molecular design},
keywords = {Algorithms,Binding Sites,Hydrogen Bonding,Hydrophobic and Hydrophilic Interactions,Ligands,Models, Molecular,Protein Binding,Proteins,Proteins: chemistry,Thermodynamics,Water,Water: chemistry},
