pages = {701--23},
pmid = {22203423},
title = {{Substantial improvements in large-scale redocking and screening using the novel HYDE scoring function.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/22203423},
volume = {26},
year = {2012}
}
