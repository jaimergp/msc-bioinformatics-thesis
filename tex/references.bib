journal = {Protein science : a publication of the Protein Society},
keywords = {Allosteric Regulation,Amino Acid Sequence,Animals,Binding Sites,Copper,Copper: chemistry,Hemocyanin,Hemocyanin: chemistry,Horseshoe Crabs,Ligands,Models, Molecular,Molecular Sequence Data,Molecular Structure,Oxygen,Oxygen: chemistry,Protein Conformation,Sequence Homology, Amino Acid,X-Ray Diffraction},
